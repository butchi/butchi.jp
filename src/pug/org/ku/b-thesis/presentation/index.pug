extends ../../../../_layout/_default-layout

append config-block
  - pageId = 'org--ku--b-thesis--presentation'
  - title = '卒業研究'
  - rootPath = '../../../../'
  - path = 'org/ku/b-thesis/'
  - swfPath = `${rootPath}swf/${path}`

//- append stylesheet-block

//- append javascript-block

block content-block
  object#b-thesis(classid='clsid:d27cdb6e-ae6d-11cf-96b8-444553540000', codebase='http://fpdownload.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=8,0,0,0', width='100%', height='100%', align='middle')
    param(name='allowScriptAccess', value='sameDomain')
    param(name='movie', value='b-thesis.swf')
    param(name='quality', value='high')
    param(name='bgcolor', value='#ffffff')
    embed(src=`${swfPath}b-thesis.swf`, quality='high', bgcolor='#ffffff', width='100%', height='100%', name='b-thesis', align='middle', allowscriptaccess='sameDomain', type='application/x-shockwave-flash', pluginspage='http://www.macromedia.com/go/getflashplayer')
